<h2>用户评论</h2>
<ul class="zdm-comment-block">

    <%_.each(comments,function(comment){%>

        <li class="z-mod">
            <div class="hd">
                <img src="http://wwc.taobaocdn.com/avatar/getAvatar.do?userId=<%=comment.raterUid%>&width=40&height=40&type=sns"/>
            </div>
            <div class="bd">
                <h3><a href="#"><%= comment.raterUserNick%></a></h3>
                <p><%= comment.feedback%></p>
                <ul class="pic-desc">
                    <%_.each(comment.feedItemPicDOList,function(pic){%>
                      <li><img src="pic.path"/></li>
                    <%})%>
                </ul>
            </div>
        </li>

    <% })%>

</ul>