<%_.each(goods,function(good){%>
    <% _.each(good.boughtItem,function(boughtItem){%>
    <li class="z-mod" id="J-commentItem-<%= boughtItem.itemId %>"  data-itemId="<%= boughtItem.itemId %>">
        <div class="good-item">
            <div class="hd good-hd">
                <img src="<%= boughtItem.pic %>"/>
            </div>
            <div class="bd good-bd">
                 <p><%= boughtItem.title %></p>
            </div>
        </div>

    </li>
    <% })%>
<%})%>

