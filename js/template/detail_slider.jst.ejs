<div id="sliderWrap" class="dSlider">
    <div class="js-bton">
        <a href="#" class="jsb-back">返 回</a>
        <a href="#" class="jsb-ori">原 图</a>
    </div>
    <div class="goods-slider" id="J-sliderShow">
        <ul>
            <% _.each(sliders,function(slider){ %>
              <li><img src="http://a.tbcdn.cn/mw/webapp/fav/img/grey.gif"  class="lazy" data-src="<%= app.Util.getWebpImg(slider,'180x180')%>" /></li>
            <%})%>
        </ul>
    </div>
    <div class="price-f"><span class="p">￥<%= info.price %></span><span class="t">运费：<% if(info.delivery.deliveryFeeType != 0){ %><% if(info.delivery.deliveryFeeType == 2){%><%= info.delivery.title %><%} else if(info.delivery.deliveryFeeType == 1){%> <%= info.delivery.deliveryFees[0].title%><%}%>
      <%}%></span></div>
    <b class="prev">上一个</b>
    <b class="next">下一个</b>
</div>